<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.doom.s1.qna.tag.QnaTagDAO">
 
 	<insert id="qnaTagInsert" parameterType="com.doom.s1.qna.tag.QnaTagVO">
 		INSERT INTO qnaTag VALUES(tag_seq.nextval, #{qna_storekey}, #{qtag_name})
 	</insert>
 	
 	<select id="qnaTagSelect" parameterType="Long" resultType="com.doom.s1.qna.tag.QnaTagVO">
 		SELECT * FROM qnaTag WHERE qna_storekey=#{qna_storekey}
 	</select>
 
 </mapper>