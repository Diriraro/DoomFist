<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.doom.s1.member.MemberDAO">
  
  	<insert id="memberJoin" parameterType="MemberVO">
		insert into member values (#{id}, #{pw}, #{email}, #{name}, #{phone})
	</insert>
	
		<select id="memberLogin" parameterType="MemberVO" resultType="MemberVO">
		select * from member where id=#{id} and pw=#{pw}
		</select>
  
  <update id="memberUpdate" parameterType="MemberVO">
		update member set name=#{email}, email=#{name}, phone=#{phone} where id=#{id}
	</update>
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  </mapper>